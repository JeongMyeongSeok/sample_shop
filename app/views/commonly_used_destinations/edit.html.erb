<div class="row">
  <div class="span6 offset3">
    <%= form_for(@address) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :address %>
      <%= f.text_field :address %>

      <%= f.label :phone %>
      <%= f.text_field :phone %>
      
      <p><%= f.submit "Save changes", class: "btn btn-large btn-primary" %></p>
    <% end %>
    
  </div>
</div>
